  </rule>

  <sequence name="MainSequence">

    <operation name="LogInfo">
      <participant name="BPLogger"/>
      <output message="BPLoggerMessage">
        <assign to="logLevel">INFO</assign>
        <assign to="message">Minimal BPML process validated successfully.</assign>
      </output>
      <input message="inmsg">
        <assign to="." from="*"/>
      </input>
    </operation>

  </sequence>

</process>